{
  "name": "thegemtrove",
  "repository": "git@github.com:theGemTrove/theGemTrove.git",
  "author": "Dustin K Greco <dkgreco@thegemtrove.tech>",
  "description": "Personal Website Presence For DKG",
  "version": "1.21.15",
  "main": "server.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:babel": "babel src/App/App.js --out-file=public/scripts/app.js --presets=env,react",
    "build:webpack": "webpack",
    "bumpVersion": "VERSION=$(npm version patch) && git tag $VERSION && git push --tags",
    "bundle:prod": "NODE_ENV=production webpack",
    "dev-server": "NODE_ENV=development webpack serve",
    "heroku-postbuild": "yarn run bundle:prod",
    "live-reload": "live-server dist",
    "publish": "yarn run bumpVersion && yarn run heroku-postbuild && git push heroku main",
    "start": "node server/server.js",
    "watch": "nodemon server/server.js",
    "watch:webpack": "webpack --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12.14.0",
    "npm": "6.14.5",
    "yarn": "1.22.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.3",
    "@fortawesome/pro-light-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^4.6.0",
    "css-loader": "^5.2.6",
    "express": "^4.17.1",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "typewriter-effect": "^2.17.0",
    "web-vitals": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "debug": "^4.3.1",
    "html-webpack-plugin": "^5.3.1",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^1.6.0",
    "sockjs-client": "^1.5.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
